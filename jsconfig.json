{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020", "DOM"],
    "checkJs": false,
    "noEmit": true,
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true
  },
  "exclude": ["node_modules", "*.ejs"],
  "include": ["**/*.js", "**/*.jsx"],
  "ts.checkJsDefaultLibLocation": true
}
